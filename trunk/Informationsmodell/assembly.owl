<?xml version="1.0"?>
<rdf:RDF
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:assem="http://localhost/ontologies/assembly.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
  xml:base="http://localhost/ontologies/assembly.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://localhost/ontologies/geometry.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="MechanicalConnection">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://localhost/ontologies/base.owl#isAttributeOf"/>
        <owl:allValuesFrom rdf:resource="http://localhost/ontologies/base.owl#Entity"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://localhost/ontologies/base.owl#RelationalAssemblyAttribute"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://localhost/ontologies/base.owl#isRealizedBy"/>
        <owl:someValuesFrom rdf:resource="http://localhost/ontologies/base.owl#PhysicalComponent"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >&lt;p style="margin-top: 0"&gt;
      Two &lt;i&gt;Entities&lt;/i&gt; that are mechanically connected have the same &lt;i&gt;MechanicalConnection&lt;/i&gt; 
      Attribute
    &lt;/p&gt;</rdfs:comment>
  </owl:Class>
</rdf:RDF>

